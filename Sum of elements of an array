import java.util.Scanner;
public class SumOfAllElementsOfAnArray {
    public static void main(String[] args) {
        
        Scanner obj=new Scanner(System.in);
        int arr[]=new int[5];
        int i,sum=0;
        for(i=0;i<5;i++)
        {
            arr[i]=obj.nextInt();
        }
        for(i=0;i<5;i++)
        {
            sum=sum+arr[i];
        }
        System.out.println("Sum of elements of array is="+" "+sum);
                
        
    }
    
}
